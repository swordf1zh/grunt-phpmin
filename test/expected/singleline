<?php

  require './vendor/autoload.php';

  $payload = array(
    "iss" => "http://this-is-not-a-comment.org",
    "aud" => 'http://example.com#not_a_comment_either',
    "iat" => 1356999524,
    "nbf" => 1357000000,
    "exp" => time() + 30
  );

  /* This is a multiline comment example.
     The expiration date in the above token payload was set to 30
     seconds from now. */


  $key = "example_key_with_'single#quotes'";
  $key2 = 'example_key_with_"double//quotes"';
	$key3 = 'example with \'escaped quotes\' #1';
	$key4 = "example with \"escaped quotes\" #2";
	$msg = 'You\'ve used this password recently. Please choose another.';


  $jwt = JWT::encode( $payload, $key );



  $allowedRoutes = array(
    '/usuarios/login',
    '/usuarios/recover',
    '/usuarios/recPass/*',
    '/usuarios/resetPass/*',
    '/test*'
    );


  $decoded = JWT::decode( $jwt, $key, array('HS256') );
  var_dump($decoded);

  /**
   * Dummy function
   * @param none
   * @return none
   */
  function dummy() {
    echo 'hello world';
  }
?>